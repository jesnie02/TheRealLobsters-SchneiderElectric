<BorderPane prefHeight="518.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.TeamsController">
   <center>
      <TilePane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" />
         </BorderPane.margin>
      </TilePane>
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="769.0" BorderPane.alignment="CENTER">
