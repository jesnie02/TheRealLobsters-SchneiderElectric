            <Insets left="50.0" right="50.0" top="50.0" />
         </BorderPane.margin>
         <children>
            <ImageView fitHeight="27.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pictures/teamLogo.png" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <Label prefHeight="18.0" prefWidth="206.0" text="Team overview">
               <padding>
                  <Insets left="8.0" />
               </padding>
            </Label>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="606.0">
               <children>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#openProjectTeamView" text="+ Create projectTeam" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets right="32.0" />
